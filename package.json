    "angularx-social-login": "^1.2.5",
    "arrive": "2.4.1",
    "bootstrap": "4.1.3",
    "bootstrap-notify": "^3.1.3",
    "buildmail": "^4.0.1",
    "chartist": "0.11.0",
    "classlist.js": "1.1.20150312",
    "compressorjs": "^1.0.3",
    "core-js": "2.6.1",
