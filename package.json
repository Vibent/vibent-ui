{
  "name": "vibent-ui",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test --watch=false",
    "build": "cross-env NODE_OPTIONS=--max_old_space_size=8192 webpack",
    "build-dev": "node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build --configuration=dev",
    "build-prod": "ng build --configuration=prod",
    "extract-i18n": "ng xi18n vibent-ui --i18n-format xlf --output-path i18n --i18n-locale en && ng run vibent-ui:xliffmerge",
    "build-dev-i18n": "for lang in en fr; do node --max_old_space_size=5048 ./node_modules/@angular/cli/bin/ng build --configuration=dev --output-path=dist-dev/$lang --i18n-file=src/i18n/messages.$lang.xlf --i18n-format=xlf --i18n-locale=$lang; done",
    "build-prod-i18n": "for lang in en fr; do ng build --configuration=prod --output-path=dist-prod/$lang --i18n-file=src/i18n/messages.$lang.xlf --i18n-format=xlf --i18n-locale=$lang; done"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "1.0.0-beta.5",
    "@angular/animations": "7.1.4",
    "@angular/cdk": "7.2.0",
